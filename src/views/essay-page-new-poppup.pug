  script.
    const reference = document.querySelector(".essay__section--references");
    const fixedCtaSection = document.querySelector(".fixed-cta");

    const toggleAbsoluteClass = () => {
      fixedCtaSection.classList.toggle("is-absolute");
    }

    const checkButtonState = () => {
      const fixedCtaTop = fixedCtaSection.getBoundingClientRect().top;
      const referenceSectionTop = reference.getBoundingClientRect().top;
      const delta = fixedCtaTop - referenceSectionTop;
      const isCtaAbsolute = fixedCtaSection.classList.contains("is-absolute");

      if (delta > 77 && !isCtaAbsolute) {
        fixedCtaSection.setAttribute("style", `top: ${fixedCtaTop + window.scrollY}px`);
        toggleAbsoluteClass();
      }

      if (fixedCtaTop > 430 && isCtaAbsolute) {
        fixedCtaSection.removeAttribute("style");
        toggleAbsoluteClass();
      }
    }

    checkButtonState();

    window.addEventListener("scroll", checkButtonState);

  script.
    const modalTrigers = document.querySelectorAll(`.js-modal-trigger`);
    const modal = document.querySelector(`.js-modal`);
    const modalCloseTriggers = document.querySelectorAll(`.js-modal-close`);
    const orderLink = document.querySelector(`.js-order-link`);

    Array.prototype.slice.call(modalTrigers).forEach(function(element) {
      element.addEventListener("click", function(e) {
        const target = e.target;
        const href = target.getAttribute("data-href");

        modal.classList.add("is-visible");
        orderLink.setAttribute("href", href);
      });
    })

    Array.prototype.slice.call(modalCloseTriggers).forEach(function(element) {
      element.addEventListener("click", function(e) {
        modal.classList.remove("is-visible");
      });
    });